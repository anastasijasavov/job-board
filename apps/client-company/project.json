{
  "name": "client-company",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/client-company/src",
  "prefix": "nbp-it-job-board",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/client-company",
        "index": "apps/client-company/src/index.html",
        "main": "apps/client-company/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/client-company/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/client-company/src/favicon.ico",
          "apps/client-company/src/assets"
        ],
        "styles": ["apps/client-company/src/styles.scss"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "client-company:build:production"
        },
        "development": {
          "browserTarget": "client-company:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "proxyConfig": "apps/client-company/proxy.conf.json"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "client-company:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "apps/client-company/**/*.ts",
          "apps/client-company/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
